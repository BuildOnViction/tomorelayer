{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "fp"
  ],
  "globals": {
    "document": true,
    "window": true,
    "localStorage": true,
    "fetch": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "react/require-default-props": 0,
    "react/jsx-one-expression-per-line": 0,
    "jsx-a11y/label-has-for": [
      1,
      {
        "required": {
          "some": [ "nesting", "id" ]
        }
      }
    ],
    "semi": 0,
    "no-debugger": 2,
    "no-console": 2,
    "camelcase": 2,
    "consistent-return": 2,
    "comma-dangle": 2,
    "jsx-quotes": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0,
    "no-confusing-arrow": 0,
    "function-paren-newline": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0
  }
}
